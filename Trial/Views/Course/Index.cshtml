@model Trial.ViewModels.CourseViewModel;
@{
    
}

<table id="myTable" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Ders Kodu</th>
                <th>Ders Adı</th>
                <th>Ders Kotası</th>
                <th>Ders Tipi</th>
                <th>Pratik Ders Saati</th>
                <th>Teorik Ders Saati</th>
                <th>Kredi</th>
                <th>AKTS</th>
                
               

            </tr>
        </thead>
        <tbody>


        @{
            if(Model.Courses != null){
            for(int i = 0; i < Model.Courses.Count; i++)
            {
                <tr>
               
                <td>@Model.Courses.ElementAt(i).CourseCode</td>
                <td>@Model.Courses.ElementAt(i).CourseName</td>
                <td>@Model.Courses.ElementAt(i).Quota</td>
                <td>@Model.Courses.ElementAt(i).CourseType</td>
                <td>@Model.Courses.ElementAt(i).PracticalHour</td>
                <td>@Model.Courses.ElementAt(i).TheoreticalHour</td>
                <td>@Model.Courses.ElementAt(i).Credit</td>
                <td>@Model.Courses.ElementAt(i).ECTS</td>
               
               
                
                </tr>
            }}

            }
        </tbody>
    </table>

