@model Trial.ViewModels.ClassScheduleViewModel;
@{
    
}

<table id="myTable" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>Class Schedule Item No</th>
                <th>Classroom Code</th>
                <th>Time Slot Id</th>
                <th>Course Offered Id</th>
               
                
               

            </tr>
        </thead>
        <tbody>


        @{
            if(Model.ClassSchedules != null){
            for(int i = 0; i < Model.ClassSchedules.Count; i++)
            {
                <tr>
               
                <td>@Model.ClassSchedules.ElementAt(i).Id</td>
                <td>@Model.ClassSchedules.ElementAt(i).ClassScheduleItemNo</td>
                <td>@Model.ClassSchedules.ElementAt(i).ClassroomCode</td>
                <td>@Model.ClassSchedules.ElementAt(i).TimeSlotId</td>
                <td>@Model.ClassSchedules.ElementAt(i).CourseOfferedId</td>
                
               
               
                
                </tr>
            }}

            }
        </tbody>
    </table>

