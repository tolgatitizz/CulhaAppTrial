@model Trial.ViewModels.DayClassScheduleViewModel;
@{
    
}

<table id="myTable" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                
                <th>Classroom Code</th>
                <th>Course Name</th>
                <th>Semester Count</th>
                <th>Department Name</th>
                <th>Academician Full Name</th>
                <th>Slot</th>
               
                
               

            </tr>
        </thead>
        <tbody>


        @{
            if(Model.DayClassSchedules != null){
            for(int i = 0; i < Model.DayClassSchedules.Count; i++)
            {
                <tr>
               
             
                <td>@Model.DayClassSchedules.ElementAt(i).ClassroomCode</td>
                <td>@Model.DayClassSchedules.ElementAt(i).CourseName</td>
                <td>@Model.DayClassSchedules.ElementAt(i).SemesterCount</td>
                <td>@Model.DayClassSchedules.ElementAt(i).DepartmentName</td>
                <td>@Model.DayClassSchedules.ElementAt(i).AcademicianFullName</td>
                <td>@Model.DayClassSchedules.ElementAt(i).Slot</td>
               
               
                
                </tr>
            }}

            }
        </tbody>
    </table>

